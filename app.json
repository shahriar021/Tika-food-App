{
  "expo": {
    "name": "Tika Food",
    "slug": "Tika-food",
    "version": "1.2.35",
    "orientation": "portrait",
    "icon": "./assets/tika.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/tika.png",
      "resizeMode": "contain",
      "backgroundColor": "#B42315"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sparktech.tikafood"
    },
    "android": {
      "versionCode": 141,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "useNextNotificationsApi": true,
      "package": "com.sparktech.tikafood"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.24",
            "composeCompilerVersion": "1.5.14"
          }
        }
      ],
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "Tika-food",
          "organization": "shaka"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a26188a5-d595-41b2-b2fb-ff7953f64a2d"
      }
    }
  }
}
